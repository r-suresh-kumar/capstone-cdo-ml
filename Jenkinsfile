pipeline {
  agent any
  stages {
    stage('Initializing') {
      steps {
        echo 'Initializing'
      }
    }

    stage('Lint HTML') {
      steps {
        echo 'Lint HTML'
        echo 'Lint HTML'
      }
    }

    stage('Build Docker Image') {
      steps {
        echo 'Build Docker Image'
      }
    }

    stage('Push to Docker Hub') {
      steps {
        echo 'Push to Docker Hub'
      }
    }

    stage('Deploy to Cluster') {
      steps {
        echo 'Deploy to Cluster'
      }
    }

    stage('Take Down') {
      steps {
        echo 'Take Down'
      }
    }

    stage('End') {
      steps {
        echo 'End'
      }
    }

  }
}